urn Math.ceil(this.totalResults / this.perPage)\n        },\n        paginatedOrders() {\n            return this.imgs.slice((this.currentPage - 1) * this.perPage, this.currentPage * this.perPage);\n        }\n    },\n    methods: {\n        showImg (index) {\n\t\t\tthis.index = index\n\t\t\tthis.visible = true\n\t\t},\n\t\thandleHide () {\n\t\t\tthis.visible = false\n\t\t},\n        onPageClick(page){\n            this.currentPage = page;\n        },\n        loadMore() {\n            this.currentPage += 1\n        }\n    }\n}\n</script>\n<style scoped>\n    .project-list {\n        background-color: #F4F5F7;\n    }\n</style>"]}]}